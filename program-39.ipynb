   "source": [
    "#frequency of characters in a given string\n",
    "s=input(\"enter a string :\")\n",
    "frq={}\n",
    "for i in s:\n",
    "    if i in frq:\n",
    "        frq[i]=frq[i]+1\n",
    "    else:\n",
    "        frq[i]=1\n",
    "for keys,value in frq.items():\n",
    "     print(keys,'is repeated',value,'times')\n",
    "    \n",
    "    "
   ]
