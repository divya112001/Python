   "source": [
    "#factorial \n",
    "n=int(input(\"Enter a number :\"))\n",
    "i=1\n",
    "f=1\n",
    "while(i<=n):\n",
    "    f=f*i\n",
    "    i=i+1\n",
    "print(\"factorial of %d is %d\"%(n,f))"
   ]
